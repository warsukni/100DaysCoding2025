public class warsukni {
    public static void main(String[] args) {
        byte umur = 18;
        short tahunlahir = 2007;
        int nimku = 0225002;
        long nohp = 6281527214544L;
        
        System.out.println("umur saya : " +umur + " tahun ");
        System.out.println("tahun lahir saya : " + tahunlahir);
        System.out.println("nim saya : " + nimku);
        System.out.println("nomor hp saya : " + nohp);
        
        // jasi huruf L dibelakang nomor hp bukan bagian dari angka, tapi cuman
        // penanda tipe data untuk java bahwa angkanya disimpan sebagai long
    }
    
}
